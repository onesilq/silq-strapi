{
  "kind": "collectionType",
  "collectionName": "tour_completions",
  "info": {
    "singularName": "tour-completion",
    "pluralName": "tour-completions",
    "displayName": "Tour Completion"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "userId": {
      "type": "string"
    },
    "userEmail": {
      "type": "email"
    },
    "tourIdentifier": {
      "type": "string"
    },
    "completionStatus": {
      "type": "enumeration",
      "enum": [
        "COMPLETED",
        "SKIPPED",
        "OPTED_OUT"
      ]
    },
    "skipCount": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "completedAt": {
      "type": "datetime"
    }
  }
}
