{
  "kind": "collectionType",
  "collectionName": "tour_completions",
  "info": {
    "singularName": "tour-completion",
    "pluralName": "tour-completions",
    "displayName": "Tour Completion"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "string"
    },
    "tour_identifier": {
      "type": "string"
    },
    "completed_at": {
      "type": "datetime"
    },
    "completion_status": {
      "type": "enumeration",
      "enum": [
        "completed",
        "skipped",
        "opted_out"
      ]
    },
    "skip_count": {
      "type": "integer",
      "default": 0,
      "min": 0
    }
  }
}
